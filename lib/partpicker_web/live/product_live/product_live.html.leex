<div class="p-10">
  <%= if Enum.count(@results) == 0 do %>
    <div class="text-white">
      Currently no parts available for <%= @tag %>
      Check back later
    </div>
  <% else %>
    <%= for result <- @results do %>
      <div class="">
        <%=  link result.title, to: result.where %>
      </div>
    <% end %>
  <% end %>
</div>
