<%= if @changeset do %>
<div>
  <%= for entry <- @uploads.csv.entries do %>
    <%= entry.client_name %> - <%= entry.progress %>%
  <% end %>
  <%= _f = form_for @changeset, "#",
    id: "build-form",
    phx_change: "validate",
    phx_submit: "save" %>
    <%= live_file_input @uploads.csv %>

    <%= submit "Save", phx_disable_with: "Saving..." %>
  </form>
</div>
<% end %>
